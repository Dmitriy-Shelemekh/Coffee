<?xml version="1.1" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                   http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">

    <changeSet id="01" author="d.shelemekh">
        <preConditions onFail="MARK_RAN">
            <tableExists tableName="coffee"/>
            <columnExists tableName="coffee" columnName="name"/>
            <columnExists tableName="coffee" columnName="create_date"/>
        </preConditions>

        <sql>
            INSERT INTO coffee (id, name, create_date)
            VALUES
                (GEN_RANDOM_UUID(), 'Ephiopia', NOW()),
                (GEN_RANDOM_UUID(), 'Brasilia', NOW());
        </sql>
    </changeSet>
</databaseChangeLog>